};



// timer interrupt
void TMR1_IRQHandler(void)
{
if(inter==1){
PB11=0; // PB11 = 0 turn on Buzzer
CLK_SysTickDelay(500000); // Delay
PB11=1; // PB11 = 1 turn off Buzzer

check_gr = 1; // change to red
green_count = 9;
red_count = 5;
inter = 0;   // clean the flag of outside interrupt signal
}

if(g_check == 1){ // green light = finish
check_gr=1; // change to red
g_check=0;
}
else if(r_check == 1){  // red light = finish
check_gr=0;
r_check=0;
}


if(check_gr == 0){
if(green_count == 9) CloseSevenSegment();
ShowSevenSegment(0, green_count);

PA12 = 1; PA13=0; PA14=1;    // green
green_count--;

if(green_count < 1){
g_check = 1; // green finish
green_count = 9; // back to initial
}
}
else{
if(red_count == 5) CloseSevenSegment();
ShowSevenSegment(3, red_count);

PA12 = 1; PA13=1; PA14=0;    // red
red_count--;

if(red_count < 1){
r_check = 1; // red finish
red_count=5;
}
}
  TIMER_ClearIntFlag(TIMER1); // ready for next interrupt
// Clear Timer1 time-out interrupt flag
}

// periodic timer interrupt
void Init_Timer1(void)
{
  TIMER_Open(TIMER1, TMR1_OPERATING_MODE, TMR1_OPERATING_FREQ);
  TIMER_EnableInt(TIMER1);
  NVIC_EnableIRQ(TMR1_IRQn);
  TIMER_Start(TIMER1);
}

// PB15 outside interrupt
void EINT1_IRQHandler(void)    
{
  GPIO_CLR_INT_FLAG(PB, BIT15); // Clear GPIO interrupt flag
inter=1;
}

void Init_EXTINT(void)
{
    // Configure EINT0 pin and enable interrupt by falling edge trigger
    GPIO_SetMode(PB, BIT14, GPIO_MODE_INPUT);
    GPIO_EnableEINT0(PB, 14, GPIO_INT_FALLING);
    NVIC_EnableIRQ(EINT0_IRQn);

    // Configure EINT1 pin and enable interrupt by rising and falling edge trigger
    GPIO_SetMode(PB, BIT15, GPIO_MODE_INPUT);
    GPIO_EnableEINT1(PB, 15, GPIO_INT_RISING); // RISING, FALLING, BOTH_EDGE, HIGH, LOW
    NVIC_EnableIRQ(EINT1_IRQn);

    // Enable interrupt de-bounce function and select de-bounce sampling cycle time
    GPIO_SET_DEBOUNCE_TIME(GPIO_DBCLKSRC_LIRC, GPIO_DBCLKSEL_64);
    GPIO_ENABLE_DEBOUNCE(PB, BIT14);
    GPIO_ENABLE_DEBOUNCE(PB, BIT15);
}


int main(void)
{
    SYS_Init();
Init_Timer1();
Init_EXTINT();
OpenSevenSegment();
init_LCD();
clear_LCD();

GPIO_SetMode(PB, BIT11, GPIO_MODE_OUTPUT); // buzzer
GPIO_SetMode(PA, BIT12, GPIO_MODE_OUTPUT);  // RGB LED
GPIO_SetMode(PA, BIT13, GPIO_MODE_OUTPUT);
GPIO_SetMode(PA, BIT14, GPIO_MODE_OUTPUT);
PA12=1;   // RGB LED initial to off
PA13=1;
PA14=1;
// PA12 = Blue    on=0 , off=1
// PA13 = Green
// PA14 = Red

while(1){

if(check_gr == 0){
if(green_count % 3 == 0){
draw_LCD(green1);
}
else if(green_count % 3 == 2){
draw_LCD(green2);
}
else if(green_count % 3 == 1){
draw_LCD(green3);
}
}
else{
draw_LCD(red1);
}
}

}

